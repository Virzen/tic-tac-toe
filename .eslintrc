{
    "rules": {
		"strict": 0,
        "semi": [
            2,
            "always"
        ],
		"linebreak-style": 2,
		"no-undef": 2,
		"quotes": 0,
		"indent": 0,
		"no-unused-vars": 0,
		"comma-dangle": 0,
		"no-console": 0
    },
    "env": {
        "es6": true,
        "browser": true
    },
    "extends": "eslint:recommended"
}
